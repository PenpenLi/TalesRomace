＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝Role＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
101 ROLE_INFO
{
	"id":100,
	"name":"哇哈哈",
	"coin":1000, //金币 (游戏币)
	"rmb":200,    //钻石（充的钱）
	"stamin":10   //体力
}

＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝team＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
301 TEAM_LIST  //卡组列表
{
	  "teams":[
        {
             "cardsAtk":200, //卡牌总攻击
             "cards":[
                {			//0项为奥义信息，没有奥义为空对象{}  (不是null)
                },
                {
                      "xid":1001,  //卡牌（奥义）数据表ID 
                      "atk":40,    //卡牌（奥义）的攻击力
                       "state":1    // 1可编辑 2不可编辑
                }
             ]
        },
}

302 TEAM_CARD //开组中卡牌信息
{
	"cards":[
		{
			"xid":1001,  //卡牌策划表的id
			"isUse":1,   //是否已经编组（使用）
			"star":3,    //星级
			"level":2    //登记
		}
	]
}

303 TEAM_JOIN //上场(下场)
{
	"team":1, //卡组id(0-11)
	"cardID":1001, //卡牌ID
	"type":1 //1上场 2下场
}

304 TEAM_UNLOCK  //解锁

＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝关卡＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
401 GATE_INFO
{
	"nodes":[
		{
			"xid":1001, //节点ID
			"star":2    //星级 没有打过星级为0
		}
	]
}