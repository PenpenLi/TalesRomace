//获取卡牌背包信息
request:
{
    ip:"123.57.6.164",//外网测试IP  gate.gateHandler.entry 返回的host
    port:6010,        //外网测试端口 gate.gateHandler.entry 返回的port
    route:"room.playerHandler.teamCard",  //消息route
    msg:{
    }
}


response:
{
    msgID:6003,          //消息ID
    code:200,           //结果 200成功  500失败
    err:1              //返回的失败枚举ID 见errEnum
    cardNum:1,
    maxNum:30,
    cards:[
        {
            cardID:1001,//卡牌ID 卡牌在背包中的唯一ID
            xid:1001,  //卡牌策划表的id
            isUse:1,   //是否已经编组（使用）
            star:3,    //星级
            level:2    //登记
        }
    ]
}
